<resources>
  <string id="appName">OTP Authenticator</string>
  <string id="appVersionTitle">Version</string>
  <string id="secret">Secret key</string>
  <string id="data">Encrypted data for backup/restore</string>

  <!-- https://www.flaticon.com/free-icon/padlock_188095 -->
  <bitmap id="LauncherIcon" filename="images/launcher_icon.png" />

  <properties>
    <property id="appVersion" type="string">2.0.0</property>
    <property id="secret" type="string"></property>
    <property id="data" type="string"></property>
  </properties>

  <settings>
    <setting propertyKey="@Properties.appVersion" title="@Strings.appVersionTitle">
      <settingConfig type="alphaNumeric" readonly="true" />
    </setting>
    <setting propertyKey="@Properties.secret" title="@Strings.secret">
      <settingConfig type="alphaNumeric" />
    </setting>
    <setting propertyKey="@Properties.data" title="@Strings.data">
      <settingConfig type="alphaNumeric" />
    </setting>
  </settings>
</resources>
